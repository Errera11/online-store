,[],"/Users/ilnaz/WebstormProjects/online-store/client/src/pages/auth/Auth.jsx",["155"],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/components/button/Button.jsx",[],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/pages/auth/signIn.jsx",[],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/pages/auth/signUp.jsx",[],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/pages/ItemShop/ItemShop.jsx",["156","157","158","159","160","161","162","163","164","165","166"],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/store/DeviceStore.js",[],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/pages/ItemShop/types/Types.jsx",[],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/components/selected/Selected.jsx",[],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/pages/ItemShop/brands/Brands.jsx",["167"],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/pages/ItemShop/items/Items.jsx",["168","169"],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/pages/itemPage/ItemPage.jsx",["170","171","172"],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/pages/admin/Admin.jsx",[],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/components/adminModal/CreateBrand.jsx",[],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/components/adminModal/CreateItem.jsx",["173","174"],[],"import React, {useContext, useState} from 'react';\nimport Button from \"../button/Button\";\nimport Modal from \"../Modal\";\nimport {Context} from \"../../index\";\nimport {postItem} from \"../../http/itemApi\";\n\n\nconst CreateItem = ({onHide, show}) => {\n    const {devices} = useContext(Context)\n    const [name, setName] = useState('')\n    const [price, setPrice] = useState('')\n    const [image, setImage] = useState()\n    const [brand, setBrand] = useState(1)\n    const [type, setType] = useState(1)\n    const [info, setInfo] = useState([[{id: new Date(), title: '', description: ''}]])\n\n    const deleteInfo = (id) => {\n        setInfo([...info.filter(item => item.id !== id)]);\n        if (info.length < 1) setInfo([{id: new Date(), title: '', description: ''}]);\n    }\n    const addInfo = () => {\n        setInfo([...info, {id: new Date(), title: '', description: ''}]);\n    }\n\n    const createItem = () => {\n        const formData = new FormData();\n        formData.append('name', name);\n        formData.append('price', price);\n        formData.append('image', image);\n        formData.append('brandId', brand.toString());\n        formData.append('typeId', type.toString());\n        formData.append('info', JSON.stringify(info.splice(0, info.length - 1)));\n        postItem(formData).then(data => {\n            onHide()\n        });\n    }\n\n    return (\n        <Modal onHide={onHide} show={show}>\n            <div className={'mb-8'}>Create Item</div>\n            <input className={'p-2 m-4 rounded-2xl'}\n                   placeholder={'Item name'}\n                   value={name}\n                   onChange={e => setName(e.target.value)}/>\n            <input className={'p-2 m-4 rounded-2xl'}\n                   placeholder={'Item price'}\n                   value={price}\n                   onChange={e => setPrice(e.target.value)}/>\n            <input className={'p-2 m-4 rounded-2xl'} type={'file'}\n                   onChange={e => setImage(e.target.files[0])}/>\n            <div className={'p-2 m-4 rounded-2xl'}>\n                <select\n                    className={'p-4 border-none w-2/3'}\n                    onChange={e => setType(e.target.value)}>\n                    {devices.types.map(type => {\n                        return <option value={type.id}>{type.name}</option>\n                    })}\n                </select>\n            </div>\n            <div className={'p-2 m-4'}>\n                <select className={'p-4 border-none w-2/3'}\n                        onChange={e => setBrand(e.target.value)}>\n                    {devices.brands.map(brand => {\n                        return <option value={brand.id}>{brand.name}</option>\n                    })}\n                </select>\n            </div>\n            <div>\n                <div>Characteristics</div>\n                <div className={'flex flex-col '}>{info.map((item, index) => {\n                    if(index + 1 == info.length) return\n                    return <div key={item.id} className={'flex flex-row m-2 justify-center'}>\n                        <div>{item.title}:      {item.description}</div>\n                        <div className={'cursor-pointer ml-2'} onClick={() => deleteInfo(item.id)}>X</div>\n                    </div>\n                })}</div>\n\n            </div>\n            <input className={'p-2 m-2 border-2 border-black rounded-s'} type={'text'} placeholder={'Info title'} onChange={e => {\n                const item = info.pop();\n                item.title = e.target.value;\n                setInfo([...info, {...item}])\n            }}/>\n            <input className={'p-2 m-2 border-2 border-black rounded-s'} type={'text'} placeholder={'Info description'}\n                   onChange={e => {\n                       const item = info.pop();\n                       item.description = e.target.value;\n                       setInfo([...info, item])\n                   }}/>\n            <Button onClick={() => addInfo()}>Add characteristic</Button>\n            <Button onClick={() => createItem()}>Create item</Button>\n        </Modal>\n    );\n};\n\nexport default CreateItem;",[],"/Users/ilnaz/WebstormProjects/online-store/client/src/components/adminModal/CreateType.jsx",[],[],"/Users/ilnaz/WebstormProjects/online-store/client/src/components/Modal.jsx",["175"],[],"import React from 'react';\nimport Button from \"./button/Button\";\n\nconst Modal = ({show, onHide, children}) => {\n    return (\n        <div className={show ? '' +\n            'w-screen mb-4 absolute' : 'hidden'}>\n            <div className={'f